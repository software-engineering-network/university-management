<UserControl x:Class="UniversityManagement.Wpf.Enrollment.ApplicationView2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:enrollment="clr-namespace:UniversityManagement.Wpf.Enrollment"
             mc:Ignorable="d"
             Height="450" Width="800">
    <StackPanel>
        <enrollment:ApplicantView2 DataContext="{Binding Applicant}" />
        <enrollment:ProgramView2 DataContext="{Binding ProgramSelector}" />
        <enrollment:ElectivesView />
    </StackPanel>
</UserControl>
