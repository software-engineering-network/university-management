<Window x:Class="UniversityManagement.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:UniversityManagement.Wpf"
        xmlns:enrollment="clr-namespace:UniversityManagement.Wpf.Enrollment"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=wpf:MainWindowViewModel}"
        Title="{Binding Path=Title}" Height="450" Width="800">
    <enrollment:CreateApplicationView />
</Window>
